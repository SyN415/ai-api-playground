# Frontend Environment Configuration for AI API Playground

# API Configuration
REACT_APP_API_BASE_URL=http://localhost:10000
REACT_APP_API_VERSION=v1
REACT_APP_API_TIMEOUT=30000

# Application Configuration
REACT_APP_ENVIRONMENT=development
REACT_APP_VERSION=1.0.0
REACT_APP_NAME=AI API Playground

# Feature Flags
REACT_APP_ENABLE_ANALYTICS=false
REACT_APP_ENABLE_ERROR_TRACKING=false
REACT_APP_ENABLE_PERFORMANCE_MONITORING=false

# Authentication Configuration
REACT_APP_AUTH_TOKEN_KEY=auth_token
REACT_APP_REFRESH_TOKEN_KEY=refresh_token
REACT_APP_USER_KEY=user_data

# UI Configuration
REACT_APP_DEFAULT_THEME=light
REACT_APP_ENABLE_DARK_MODE=true
REACT_APP_ENABLE_NOTIFICATIONS=true
REACT_APP_NOTIFICATION_DURATION=5000

# API Endpoints
REACT_APP_AUTH_ENDPOINT=/api/v1/auth
REACT_APP_AI_ENDPOINT=/api/v1/ai
REACT_APP_WEBHOOK_ENDPOINT=/api/v1/webhooks
REACT_APP_ADMIN_ENDPOINT=/api/v1/admin

# Minimax Configuration
REACT_APP_MINIMAX_ENABLED=true
REACT_APP_MINIMAX_DEFAULT_MODEL=abab5.5-chat
REACT_APP_MINIMAX_MAX_TOKENS=8192

# Rate Limiting Configuration
REACT_APP_RATE_LIMIT_WARNING_THRESHOLD=80
REACT_APP_RATE_LIMIT_ERROR_THRESHOLD=95

# Webhook Configuration
REACT_APP_WEBHOOK_MAX_PER_USER=10
REACT_APP_WEBHOOK_TIMEOUT=10000

# Error Handling
REACT_APP_ERROR_REPORTING_ENDPOINT=/api/v1/errors
REACT_APP_MAX_ERROR_REPORTS_PER_SESSION=5

# Performance Configuration
REACT_APP_ENABLE_LAZY_LOADING=true
REACT_APP_ENABLE_CODE_SPLITTING=true
REACT_APP_IMAGE_OPTIMIZATION=true

# Development Configuration
REACT_APP_ENABLE_MOCK_DATA=false
REACT_APP_MOCK_API_DELAY=500
REACT_APP_ENABLE_DEBUG_TOOLS=false

# Security Configuration
REACT_APP_ENABLE_CSP=true
REACT_APP_ENABLE_XSS_PROTECTION=true
REACT_APP_ENABLE_HTTPS_ONLY=false

# External Services
REACT_APP_GOOGLE_ANALYTICS_ID=
REACT_APP_SENTRY_DSN=
REACT_APP_LOGROCKET_ID=

# Build Configuration
REACT_APP_GENERATE_SOURCEMAP=false
REACT_APP_INLINE_RUNTIME_CHUNK=false
REACT_APP_IMAGE_INLINE_SIZE_LIMIT=10000

# PWA Configuration
REACT_APP_ENABLE_PWA=false
REACT_APP_SERVICE_WORKER_FILE=service-worker.js

# Testing Configuration
REACT_APP_ENABLE_TEST_MODE=false
REACT_APP_TEST_USER_EMAIL=test@example.com
REACT_APP_TEST_USER_PASSWORD=test123